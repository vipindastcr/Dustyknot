    <script>
        function select() {
            let selectedOption = document.getElementById('mySelect').value;
            console.log("selectedOption : ",selectedOption);

            if (selectedOption) {
            
            switch (selectedOption) {
                case 'option1':
                    window.location.href = '/category?sort=lowtohigh';
                    break;
                case 'option2':
                    window.location.href = '/category?sort=hightolow';
                    break;
                case 'option3':
                    window.location.href = '/category?sort=aAzZ';
                    break;
                case 'option4':
                    window.location.href = '/category?sort=zZaA';
                    break;
                default:
                    break;
            }
        }
        }
    </script>

    
<script>
    function redirectToCategory(categoryID){
        var selectedCat = categoryID.value;
        console.log('the selectedCat is.........................',selectedCat);
        if(selectedCat !== "All Categories"){
        window.location.href = `/XXXy?id=${categoryID}`+ encodeURIComponent(selectedCat);
        }else {
            window.location.href = "/Categor"
        }
    }
</script>
    

    

